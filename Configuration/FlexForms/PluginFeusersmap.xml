<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<sheetTitle>
					LLL:EXT:feusersmap/Resources/Private/Language/locallang_db.xlf:tx_feusersmap.pi_flexform.sheetGeneral
				</sheetTitle>
				<type>array</type>
				<el>
					<settings.showFilter>
						<TCEforms>
							<label>
								LLL:EXT:feusersmap/Resources/Private/Language/locallang_db.xlf:tx_feusersmap.pi_flexform.showFilter
							</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showFilter>
					<settings.categories>
						<TCEforms>
							<label>
								LLL:EXT:feusersmap/Resources/Private/Language/locallang_db.xlf:tx_feusersmap.pi_flexform.showCustomGroups
							</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>fe_groups</allowed>
								<size>4</size>
								<maxitems>999</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</settings.categories>
					<settings.singlePid>
						<label>
							LLL:EXT:feusersmap/Resources/Private/Language/locallang_db.xlf:tx_feusersmap.pi_flexform.singlePageUid
						</label>
						<config>
							<type>group</type>
							<allowed>pages</allowed>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<size>1</size>
							<suggestOptions>
								<default>
									<additionalSearchFields>nav_title, alias, url</additionalSearchFields>
									<addWhere>AND pages.doktype = 1</addWhere>
								</default>
							</suggestOptions>
						</config>
					</settings.singlePid>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
