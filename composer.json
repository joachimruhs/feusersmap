{
  "name": "wsr/feusersmap",
  "description": "Openstreetmap / leaflet maps with radial search for fe_users and fe_groups.",
  "keywords": ["TYPO3", "Openstreet map", "extension", "radial search"],
  "homepage": "https://www.web-services-ruhs.de",
  "authors": [
    {
      "name": "Joachim Ruhs",
      "email": "postmaster@joachim-ruhs.de"
    }
  ],
  "license": "MIT",
  "type": "typo3-cms-extension",
  "require": {
	"typo3/cms-core": "^11.5 || ^12",
	"typo3/cms-frontend": "^11.5 || ^12"
  },
  "replace": {
    "typo3-ter/feusersmap": "self.version"
  },
  "autoload": {
    "psr-4": {
      "WSR\\Feusersmap\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
       "web-dir": ".Build/public",
       "extension-key": "feusersmap"
    }
  }
}